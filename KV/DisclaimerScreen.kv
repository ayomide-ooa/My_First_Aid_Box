<DisclaimerScreen>:
    name: 'disclaimer'

    ScrollView:
        do_scroll_x: True
        effect_cls: 'ScrollEffect'
        bar_width: 0
        MDBoxLayout:
            adaptive_height: True
            spacing: 40
            orientation: 'vertical'

            MDLabel:
                text: root.text
                markup: True
                font_size: '14dp'
                halign: 'justify'
                color: app.theme_cls.secondaryColor
                pos_hint: {'center_y': .5}
                font_name: 'T1Robit'
                theme_font_name: 'Primary'
                theme_font_size: 'Custom'
                padding: ('20dp')
                adaptive_height: True
                size: self.texture_size
                text_size: root.width, None


            MDBoxLayout:
                orientation: 'horizontal'
                spacing: '30dp'
                pos_hint: {'center_x': .5}
                adaptive_width: True

                MDButton:
                    style: 'outlined'
                    on_release: app.change_screen('welcome', 'right')
                    outline_color: app.theme_cls.primaryColor

                    MDButtonText:
                        text: 'Cancel'.upper()
                        bold: True
#                        pos_hint: {'center_x': .5, 'center_y': .5}

                MDButton:
                    style: 'filled'
                    on_release: app.change_screen('layout', 'left', app.load_symptoms_chip())
                    theme_elevation_level: 'Custom'
                    elevation_level: 4
                    theme_shadow_softness: 'Custom'
                    shadow_softness: 8
                    theme_shadow_offset: 'Custom'
                    shadow_offset: 0, -4
                    font_name: 'T1Robit'

                    MDButtonText:
                        text: 'I Understand'.upper()
                        bold: True

<UnderLayout>:
    name: 'layout'

